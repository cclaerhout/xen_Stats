    <template title="bbm_stats_jqplot.css" version_id="0" version_string="1.0"><![CDATA[.bbmStatsContainer{
	overflow:auto;
}

.bbmStatsContainer > .stats.bleft {
	margin-right:auto;
}

.bbmStatsContainer > .stats.bcenter {
	margin-left:auto;
	margin-right:auto;
}

.bbmStatsContainer > .stats.bright {
	margin-left:auto;
}

.bbmStatsContainer.fright{
	float:right;
	margin-left: 8px;
	margin-right: 4px;
}

.bbmStatsContainer.fleft{
	float:left;
	margin-left: 4px;
	margin-right: 8px;
}

.bbmStatsContainer .jqplot-target {
	position: relative;
	color: #666666;
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
	font-size: 1em;
	
	min-width: {xen:property bbm_stats_minwidth};
	max-width: {xen:property bbm_stats_maxwidth};
	width: {xen:property bbm_stats_defaultwidth};
	
	max-height: {xen:property bbm_stats_maxheight};
	height: {xen:property bbm_stats_defaultheight};
}

/*rules applied to all axes*/
.bbmStatsContainer .jqplot-axis {
	font-size: 0.75em;
}

.bbmStatsContainer .jqplot-xaxis {
	margin-top: 10px;
}

.bbmStatsContainer .jqplot-x2axis {
	margin-bottom: 10px;
}

.bbmStatsContainer .jqplot-yaxis {
	margin-right: 10px;
}

.bbmStatsContainer .jqplot-y2axis, .jqplot-y3axis, .jqplot-y4axis, .jqplot-y5axis, .jqplot-y6axis, .jqplot-y7axis, .jqplot-y8axis, .jqplot-y9axis, .jqplot-yMidAxis {
	margin-left: 10px;
	margin-right: 10px;
}

/*rules applied to all axis tick divs*/
.bbmStatsContainer .jqplot-axis-tick, .jqplot-xaxis-tick, .jqplot-yaxis-tick, .jqplot-x2axis-tick, .jqplot-y2axis-tick, .jqplot-y3axis-tick, .jqplot-y4axis-tick, .jqplot-y5axis-tick, .jqplot-y6axis-tick, .jqplot-y7axis-tick, .jqplot-y8axis-tick, .jqplot-y9axis-tick, .jqplot-yMidAxis-tick {
	position: absolute;
	white-space: pre;
}

.bbmStatsContainer .jqplot-xaxis-tick {
	top: 0px;
	/* initial position untill tick is drawn in proper place */
	left: 15px;
/*	padding-top: 10px;*/
	vertical-align: top;
}

.bbmStatsContainer .jqplot-x2axis-tick {
	bottom: 0px;
	/* initial position untill tick is drawn in proper place */
	left: 15px;
/*	padding-bottom: 10px;*/
	vertical-align: bottom;
}

.bbmStatsContainer .jqplot-yaxis-tick {
	right: 0px;
	/* initial position untill tick is drawn in proper place */
	top: 15px;
/*	padding-right: 10px;*/
	text-align: right;
}

.bbmStatsContainer .jqplot-yaxis-tick.jqplot-breakTick {
	right: -20px;
	margin-right: 0px;
	padding:1px 5px 1px 5px;
	/*background-color: white;*/
	z-index: 2;
	font-size: 1.5em;
}

.bbmStatsContainer .jqplot-y2axis-tick, .jqplot-y3axis-tick, .jqplot-y4axis-tick, .jqplot-y5axis-tick, .jqplot-y6axis-tick, .jqplot-y7axis-tick, .jqplot-y8axis-tick, .jqplot-y9axis-tick {
	left: 0px;
	/* initial position untill tick is drawn in proper place */
	top: 15px;
/*	padding-left: 10px;*/
/*	padding-right: 15px;*/
	text-align: left;
}

.bbmStatsContainer .jqplot-yMidAxis-tick {
	text-align: center;
	white-space: nowrap;
}

.bbmStatsContainer .jqplot-xaxis-label {
	margin-top: 10px;
	font-size: 11pt;
	position: absolute;
}

.bbmStatsContainer .jqplot-x2axis-label {
	margin-bottom: 10px;
	font-size: 11pt;
	position: absolute;
}

.bbmStatsContainer .jqplot-yaxis-label {
	margin-right: 10px;
/*	text-align: center;*/
	font-size: 11pt;
	position: absolute;
}

.bbmStatsContainer .jqplot-yMidAxis-label {
	font-size: 11pt;
	position: absolute;
}

.bbmStatsContainer .jqplot-y2axis-label, .jqplot-y3axis-label, .jqplot-y4axis-label, .jqplot-y5axis-label, .jqplot-y6axis-label, .jqplot-y7axis-label, .jqplot-y8axis-label, .jqplot-y9axis-label {
/*	text-align: center;*/
	font-size: 11pt;
	margin-left: 10px;
	position: absolute;
}

.bbmStatsContainer .jqplot-meterGauge-tick {
	font-size: 0.75em;
	color: #999999;
}

.bbmStatsContainer .jqplot-meterGauge-label {
	font-size: 1em;
	color: #999999;
}

.bbmStatsContainer table.jqplot-table-legend {
	margin-top: 12px;
	margin-bottom: 12px;
	margin-left: 12px;
	margin-right: 12px;
}

.bbmStatsContainer table.jqplot-table-legend, table.jqplot-cursor-legend {
	background-color: rgba(255,255,255,0.6);
	border: 1px solid #cccccc;
	position: absolute;
	font-size: 0.75em;
}

.bbmStatsContainer td.jqplot-table-legend {
	vertical-align:middle;
}

/*
These rules could be used instead of assigning
element styles and relying on js object properties.
*/

/*
.bbmStatsContainer td.jqplot-table-legend-swatch {
	padding-top: 0.5em;
	text-align: center;
}

.bbmStatsContainer tr.jqplot-table-legend:first td.jqplot-table-legend-swatch {
	padding-top: 0px;
}
*/

.bbmStatsContainer td.jqplot-seriesToggle:hover, td.jqplot-seriesToggle:active {
	cursor: pointer;
}

.bbmStatsContainer .jqplot-table-legend .jqplot-series-hidden {
	text-decoration: line-through;
}

.bbmStatsContainer div.jqplot-table-legend-swatch-outline {
	border: 1px solid #cccccc;
	padding:1px;
}

.bbmStatsContainer div.jqplot-table-legend-swatch {
	width:0px;
	height:0px;
	border-top-width: 5px;
	border-bottom-width: 5px;
	border-left-width: 6px;
	border-right-width: 6px;
	border-top-style: solid;
	border-bottom-style: solid;
	border-left-style: solid;
	border-right-style: solid;
}

.bbmStatsContainer .jqplot-title {
	{xen:property bbm_stats_title}
}

.bbmStatsContainer table.jqplot-cursor-tooltip {
	border: 1px solid #cccccc;
	font-size: 0.75em;
}

.bbmStatsContainer .jqplot-cursor-tooltip {
	border: 1px solid #cccccc;
	font-size: 0.75em;
	white-space: nowrap;
	background: rgba(208,208,208,0.5);
	padding: 1px;
}

.bbmStatsContainer .jqplot-highlighter-tooltip, .jqplot-canvasOverlay-tooltip {
	border: 1px solid #cccccc;
	font-size: 0.75em;
	white-space: nowrap;
	background: rgba(208,208,208,0.5);
	padding: 1px;
}

.bbmStatsContainer .jqplot-point-label {
	font-size: 0.75em;
	z-index: 2;
}
	  
.bbmStatsContainer td.jqplot-cursor-legend-swatch {
	vertical-align: middle;
	text-align: center;
}

.bbmStatsContainer div.jqplot-cursor-legend-swatch {
	width: 1.2em;
	height: 0.7em;
}

.bbmStatsContainer .jqplot-error {
/*   Styles added to the plot target container when there is an error go here.*/
	text-align: center;
}

.bbmStatsContainer .jqplot-error-message {
/*	Styling of the custom error message div goes here.*/
	position: relative;
	top: 46%;
	display: inline-block;
}

.bbmStatsContainer div.jqplot-bubble-label {
	font-size: 0.8em;
/*	background: rgba(90%, 90%, 90%, 0.15);*/
	padding-left: 2px;
	padding-right: 2px;
	color: rgb(20%, 20%, 20%);
}

.bbmStatsContainer div.jqplot-bubble-label.jqplot-bubble-label-highlight {
	background: rgba(90%, 90%, 90%, 0.7);
}

.bbmStatsContainer div.jqplot-noData-container {
	text-align: center;
	background-color: rgba(96%, 96%, 96%, 0.3);
}

.bbmStatsContainer table{
	border-collapse: separate;
	border-spacing: 2px;
}
.bbmStatsContainer td{
	border: 0;
	padding: 0;
}

.bbmStatsContainer .jqplot-data-label{
	{xen:property bbm_stats_data_label}
}

.bbmStatsContainer .jqplot-table-legend{
	{xen:property bbm_stats_legend_table}
}]]></template>
